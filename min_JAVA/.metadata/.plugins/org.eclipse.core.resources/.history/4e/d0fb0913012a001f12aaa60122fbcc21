	public static void main(String[] args) {
		System.out.println("게시판 프로그램 시작");
		
		// 게시판 제목
		System.out.println("                      << 게시판 >>");
		
		// 게시판 메뉴
		System.out.println("(1. 게시판 글작성, 2. 상세보기, 3. 수정, 4. 삭제, 5. 종료)");
		
		// 게시판 목록
		System.out.println("번호			제목								작성일");
		System.out.println("-------------------------------------------------");
		
		
		
		
	}
}
